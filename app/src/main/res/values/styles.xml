<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/anaranjado</item>
        <item name="colorPrimaryDark">@color/anaranjado</item>
        <item name="colorAccent">@color/anaranjado</item>
        <item name="android:textColorPrimary">@color/blanco</item>
        <item name="android:textColorSecondary">@color/anaranjado</item>
        <item name="android:textColor">@color/anaranjado</item>

    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:logo">@mipmap/banner_aida</item>
    </style>



    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />



</resources>
